import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		System.out.println("Leap year program :");
		
		int year=2004;
		
		if((year%400 ==0 ) | (year %4 ==0 && year% 100!=0 )){
		    System.out.println("Leap year");
		    
		}
		else 
		System.out.println("Not leap year");
		
	
		       
	}
}
